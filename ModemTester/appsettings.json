{
  "Modem": {
    "ComPort": "COM6",
    "BaudRate": 115200,
    "Parity": "None",
    "DataBits": 8,
    "StopBits": "One",
    "InitCommands": [
      "ATZ",
      "AT+VCID=1"
    ],
    "ReadTimeoutMs": 500,
    "WriteTimeoutMs": 500
  },

  "Lookup": {
    "Provider": "SearchCh",
    // Request your API key at https://www.search.ch/api
    "SearchChApiKey": "<PUT_YOUR_KEY_HERE>",
    "CountryCode": "CH",
    "Enable": true
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/modem_searchch_log.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 3,
          "fileSizeLimitBytes": 10485760,
          "rollOnFileSizeLimit": true
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ]
  }
}
